{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "swagger": "node ./swagger.js"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.19.4",
    "@ant-design/pro-table": "^2.41.0",
    "@dylan92/nemo": "^0.0.6",
    "@types/qs": "^6.9.6",
    "@umijs/preset-react": "1.x",
    "ahooks": "^2.10.6",
    "handlebars": "^4.7.7",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.1",
    "qs": "^6.10.1",
    "sandfish": "^0.0.1",
    "umi": "^3.4.25"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.4.25",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
